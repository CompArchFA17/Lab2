#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x161d450 .scope module, "testshiftregister" "testshiftregister" 2 7;
 .timescale 0 0;
v0x165d050_0 .var "clk", 0 0;
v0x165d0f0_0 .var "parallelDataIn", 7 0;
v0x165d1a0_0 .net "parallelDataOut", 7 0, L_0x165d510; 1 drivers
v0x165d250_0 .var "parallelLoad", 0 0;
v0x165d330_0 .var "peripheralClkEdge", 0 0;
v0x165d3e0_0 .var "serialDataIn", 0 0;
v0x165d460_0 .net "serialDataOut", 0 0, L_0x165d5c0; 1 drivers
S_0x160b690 .scope module, "dut" "shiftregister" 2 19, 3 9, S_0x161d450;
 .timescale 0 0;
P_0x1643238 .param/l "width" 3 10, +C4<01000>;
L_0x165d510 .functor BUFZ 8, v0x165cf60_0, C4<00000000>, C4<00000000>, C4<00000000>;
v0x1640e60_0 .net "clk", 0 0, v0x165d050_0; 1 drivers
v0x165cb50_0 .net "parallelDataIn", 7 0, v0x165d0f0_0; 1 drivers
v0x165cbf0_0 .alias "parallelDataOut", 7 0, v0x165d1a0_0;
v0x165cc90_0 .net "parallelLoad", 0 0, v0x165d250_0; 1 drivers
v0x165cd40_0 .net "peripheralClkEdge", 0 0, v0x165d330_0; 1 drivers
v0x165cde0_0 .net "serialDataIn", 0 0, v0x165d3e0_0; 1 drivers
v0x165cec0_0 .alias "serialDataOut", 0 0, v0x165d460_0;
v0x165cf60_0 .var "shiftregistermem", 7 0;
E_0x161d680 .event posedge, v0x1640e60_0;
L_0x165d5c0 .part v0x165cf60_0, 7, 1;
    .scope S_0x160b690;
T_0 ;
    %wait E_0x161d680;
    %load/v 8, v0x165cc90_0, 1;
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_0.0, 4;
    %load/v 8, v0x165cb50_0, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v0x165cf60_0, 0, 8;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0x165cc90_0, 1;
    %mov 9, 0, 1;
    %cmpi/u 8, 0, 2;
    %jmp/0xz  T_0.2, 4;
    %load/v 8, v0x165cd40_0, 1;
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_0.4, 4;
    %load/v 8, v0x165cde0_0, 1;
    %load/v 9, v0x165cf60_0, 7; Select 7 out of 8 bits
    %ix/load 0, 8, 0;
    %assign/v0 v0x165cf60_0, 0, 8;
T_0.4 ;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x161d450;
T_1 ;
    %set/v v0x165d050_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x161d450;
T_2 ;
    %delay 10, 0;
    %load/v 8, v0x165d050_0, 1;
    %inv 8, 1;
    %set/v v0x165d050_0, 8, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x161d450;
T_3 ;
    %vpi_call 2 33 "$dumpfile", "shiftregister.vcd";
    %vpi_call 2 34 "$dumpvars";
    %vpi_call 2 39 "$display", "PIn? | PDataIn | SDataOut";
    %set/v v0x165d250_0, 1, 1;
    %movi 8, 31, 8;
    %set/v v0x165d0f0_0, 8, 8;
    %delay 21, 0;
    %vpi_call 2 41 "$display", "%b   | %b | %b ", v0x165d250_0, v0x165d0f0_0, v0x165d460_0;
    %set/v v0x165d250_0, 1, 1;
    %movi 8, 62, 8;
    %set/v v0x165d0f0_0, 8, 8;
    %delay 21, 0;
    %vpi_call 2 43 "$display", "%b   | %b | %b", v0x165d250_0, v0x165d0f0_0, v0x165d460_0;
    %set/v v0x165d250_0, 1, 1;
    %movi 8, 125, 8;
    %set/v v0x165d0f0_0, 8, 8;
    %delay 21, 0;
    %vpi_call 2 45 "$display", "%b   | %b | %b", v0x165d250_0, v0x165d0f0_0, v0x165d460_0;
    %set/v v0x165d250_0, 1, 1;
    %movi 8, 250, 8;
    %set/v v0x165d0f0_0, 8, 8;
    %delay 21, 0;
    %vpi_call 2 47 "$display", "%b   | %b | %b", v0x165d250_0, v0x165d0f0_0, v0x165d460_0;
    %set/v v0x165d250_0, 1, 1;
    %movi 8, 244, 8;
    %set/v v0x165d0f0_0, 8, 8;
    %delay 500, 0;
    %vpi_call 2 49 "$display", "%b   | %b | %b", v0x165d250_0, v0x165d0f0_0, v0x165d460_0;
    %vpi_call 2 54 "$display", "PIn? | SDataIn | PDataOut";
    %set/v v0x165d250_0, 0, 1;
    %set/v v0x165d3e0_0, 1, 1;
    %set/v v0x165d330_0, 1, 1;
    %delay 21, 0;
    %vpi_call 2 56 "$display", "%b    | \011%b      | %b ", v0x165d250_0, v0x165d3e0_0, v0x165d1a0_0;
    %set/v v0x165d250_0, 0, 1;
    %set/v v0x165d3e0_0, 1, 1;
    %set/v v0x165d330_0, 1, 1;
    %delay 21, 0;
    %vpi_call 2 58 "$display", "%b    | \011%b      | %b ", v0x165d250_0, v0x165d3e0_0, v0x165d1a0_0;
    %set/v v0x165d250_0, 0, 1;
    %set/v v0x165d3e0_0, 0, 1;
    %set/v v0x165d330_0, 1, 1;
    %delay 21, 0;
    %vpi_call 2 60 "$display", "%b    | \011%b      | %b ", v0x165d250_0, v0x165d3e0_0, v0x165d1a0_0;
    %set/v v0x165d250_0, 0, 1;
    %set/v v0x165d3e0_0, 0, 1;
    %set/v v0x165d330_0, 1, 1;
    %delay 21, 0;
    %vpi_call 2 62 "$display", "%b    | \011%b      | %b ", v0x165d250_0, v0x165d3e0_0, v0x165d1a0_0;
    %set/v v0x165d250_0, 0, 1;
    %set/v v0x165d3e0_0, 0, 1;
    %set/v v0x165d330_0, 1, 1;
    %delay 21, 0;
    %vpi_call 2 64 "$display", "%b    | \011%b      | %b ", v0x165d250_0, v0x165d3e0_0, v0x165d1a0_0;
    %set/v v0x165d250_0, 0, 1;
    %set/v v0x165d3e0_0, 1, 1;
    %set/v v0x165d330_0, 1, 1;
    %delay 21, 0;
    %vpi_call 2 66 "$display", "%b    | \011%b      | %b ", v0x165d250_0, v0x165d3e0_0, v0x165d1a0_0;
    %vpi_call 2 69 "$finish";
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
