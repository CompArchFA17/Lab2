    %wait E_0x1899680;
    %load/v 8, v0x18d8d00_0, 1;
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_0.0, 4;
    %load/v 8, v0x18d8bc0_0, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v0x18d8fd0_0, 0, 8;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0x18d8d00_0, 1;
    %mov 9, 0, 1;
    %cmpi/u 8, 0, 2;
    %jmp/0xz  T_0.2, 4;
    %load/v 8, v0x18d8db0_0, 1;
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_0.4, 4;
    %load/v 8, v0x18d8e50_0, 1;
    %load/v 9, v0x18d8fd0_0, 7; Select 7 out of 8 bits
    %ix/load 0, 8, 0;
    %assign/v0 v0x18d8fd0_0, 0, 8;
T_0.4 ;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x1899450;
T_1 ;
    %set/v v0x18d90c0_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x1899450;
T_2 ;
