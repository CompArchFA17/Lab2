#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1241730 .scope module, "testConditioner" "testConditioner" 2 6;
 .timescale 0 0;
v0x128db00_0 .var "clk", 0 0;
v0x128dba0_0 .net "conditioned", 0 0, v0x128d600_0; 1 drivers
v0x128dc50_0 .net "falling", 0 0, v0x128d740_0; 1 drivers
v0x128dd00_0 .var "pin", 0 0;
v0x128dde0_0 .net "rising", 0 0, v0x128d890_0; 1 drivers
S_0x1241820 .scope module, "dut" "inputconditioner" 2 15, 3 9, S_0x1241730;
 .timescale 0 0;
P_0x12423c8 .param/l "counterwidth" 3 18, +C4<011>;
P_0x12423f0 .param/l "waittime" 3 19, +C4<011>;
v0x12422c0_0 .net "clk", 0 0, v0x128db00_0; 1 drivers
v0x128d600_0 .var "conditioned", 0 0;
v0x128d6a0_0 .var "counter", 2 0;
v0x128d740_0 .var "negativeedge", 0 0;
v0x128d7f0_0 .net "noisysignal", 0 0, v0x128dd00_0; 1 drivers
v0x128d890_0 .var "positiveedge", 0 0;
v0x128d970_0 .var "synchronizer0", 0 0;
v0x128da10_0 .var "synchronizer1", 0 0;
E_0x1255c30 .event posedge, v0x12422c0_0;
    .scope S_0x1241820;
T_0 ;
    %set/v v0x128d6a0_0, 0, 3;
    %end;
    .thread T_0;
    .scope S_0x1241820;
T_1 ;
    %set/v v0x128d970_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x1241820;
T_2 ;
    %set/v v0x128da10_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x1241820;
T_3 ;
    %wait E_0x1255c30;
    %load/v 8, v0x128d600_0, 1;
    %load/v 9, v0x128da10_0, 1;
    %cmp/u 8, 9, 1;
    %jmp/0xz  T_3.0, 4;
    %ix/load 0, 3, 0;
    %assign/v0 v0x128d6a0_0, 0, 0;
    %jmp T_3.1;
T_3.0 ;
    %load/v 8, v0x128d6a0_0, 3;
    %mov 11, 0, 1;
    %cmpi/u 8, 3, 4;
    %jmp/0xz  T_3.2, 4;
    %ix/load 0, 3, 0;
    %assign/v0 v0x128d6a0_0, 0, 0;
    %load/v 8, v0x128da10_0, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v0x128d600_0, 0, 8;
    %load/v 8, v0x128d600_0, 1;
    %mov 9, 0, 1;
    %cmpi/u 8, 0, 2;
    %mov 8, 4, 1;
    %load/v 9, v0x128da10_0, 1;
    %mov 10, 0, 2;
    %cmpi/u 9, 1, 3;
    %mov 9, 4, 1;
    %and 8, 9, 1;
    %jmp/0xz  T_3.4, 8;
    %ix/load 0, 1, 0;
    %assign/v0 v0x128d890_0, 0, 1;
    %vpi_call 3 35 "$display", "positive edge gotten";
T_3.4 ;
    %load/v 8, v0x128d600_0, 1;
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %mov 8, 4, 1;
    %load/v 9, v0x128da10_0, 1;
    %mov 10, 0, 1;
    %cmpi/u 9, 0, 2;
    %mov 9, 4, 1;
    %and 8, 9, 1;
    %jmp/0xz  T_3.6, 8;
    %ix/load 0, 1, 0;
    %assign/v0 v0x128d740_0, 0, 1;
    %vpi_call 3 39 "$display", "negative edge gotten";
T_3.6 ;
    %jmp T_3.3;
T_3.2 ;
    %load/v 8, v0x128d6a0_0, 3;
    %mov 11, 0, 29;
    %addi 8, 1, 32;
    %ix/load 0, 3, 0;
    %assign/v0 v0x128d6a0_0, 0, 8;
T_3.3 ;
    %vpi_call 3 44 "$display", "counter iterated";
T_3.1 ;
    %load/v 8, v0x128d890_0, 1;
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_3.8, 4;
    %ix/load 0, 1, 0;
    %assign/v0 v0x128d890_0, 0, 0;
T_3.8 ;
    %load/v 8, v0x128d740_0, 1;
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_3.10, 4;
    %ix/load 0, 1, 0;
    %assign/v0 v0x128d740_0, 0, 0;
T_3.10 ;
    %load/v 8, v0x128d7f0_0, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v0x128d970_0, 0, 8;
    %load/v 8, v0x128d970_0, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v0x128da10_0, 0, 8;
    %jmp T_3;
    .thread T_3;
    .scope S_0x1241730;
T_4 ;
    %set/v v0x128db00_0, 0, 1;
    %end;
    .thread T_4;
    .scope S_0x1241730;
T_5 ;
    %delay 10, 0;
    %load/v 8, v0x128db00_0, 1;
    %inv 8, 1;
    %set/v v0x128db00_0, 8, 1;
    %jmp T_5;
    .thread T_5;
    .scope S_0x1241730;
T_6 ;
    %vpi_call 2 25 "$dumpfile", "inputconditioner.vcd";
    %vpi_call 2 26 "$dumpvars";
    %set/v v0x128dd00_0, 0, 1;
    %delay 50, 0;
    %set/v v0x128dd00_0, 1, 1;
    %delay 160, 0;
    %set/v v0x128dd00_0, 0, 1;
    %delay 70, 0;
    %set/v v0x128dd00_0, 1, 1;
    %delay 10, 0;
    %set/v v0x128dd00_0, 0, 1;
    %delay 80, 0;
    %set/v v0x128dd00_0, 1, 1;
    %delay 60, 0;
    %set/v v0x128dd00_0, 0, 1;
    %delay 100, 0;
    %set/v v0x128dd00_0, 1, 1;
    %delay 65, 0;
    %set/v v0x128dd00_0, 0, 1;
    %delay 100, 0;
    %set/v v0x128dd00_0, 1, 1;
    %delay 70, 0;
    %set/v v0x128dd00_0, 0, 1;
    %delay 150, 0;
    %vpi_call 2 77 "$finish";
    %end;
    .thread T_6;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "inputconditioner.t.v";
    "./inputconditioner.v";
