    %pad/u 32;
    %cmpi/ne 1, 0, 32;
    %jmp/0xz  T_9.4, 4;
    %vpi_call 2 89 "$display", "output did not change on on debounce test" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x178a740_0, 0, 1;
T_9.4 ;
    %load/vec4 v0x178a740_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_9.6, 8;
