:vpi_module "va_math";
S_0x17656b0 .scope module, "testConditioner" "testConditioner" 2 6;
 .timescale 0 0;
v0x178a580_0 .var "clk", 0 0;
v0x178a670_0 .net "conditioned", 0 0, v0x1789f20_0;  1 drivers
v0x178a740_0 .var "dutpassed", 0 0;
v0x178a810_0 .net "falling", 0 0, v0x178a0d0_0;  1 drivers
v0x178a8e0_0 .var "i", 31 0;
v0x178a9d0_0 .var "pin", 0 0;
v0x178aa70_0 .net "rising", 0 0, v0x178a2a0_0;  1 drivers
